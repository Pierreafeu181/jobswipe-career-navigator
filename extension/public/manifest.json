{
  "manifest_version": 3,
  "name": "JobSwipe Career Navigator",
  "description": "A companion extension for JobSwipe to help you with your job search.",
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "all_frames": true
    }
  ]
}